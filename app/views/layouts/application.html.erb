<!DOCTYPE html>
<html>
<head>
  <title>Brewerlog</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
  <%= render :partial => "layouts/analytics" %>
</head>
<body>
	
<div id="leaderboard_top">
	<%= render :partial => "layouts/leaderboard_top" %>
</div>

<div id="headernav">
	<header id="headernavheader">
		<%= render :partial => "layouts/header" %>
	</header>

	<nav>
		<%= render :partial => "layouts/mainnav" %>
	</nav>
</div>

<section id="content">
	<%= yield %>
</section>

<footer>
	<%= render :partial => "layouts/footer" %>
</footer>

<p class="notice"><%= notice %></p>
<p class="alert"><%= alert %></p>

</body>
</html>
